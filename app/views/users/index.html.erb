
<p id="notice"><%= notice %></p>

<h1>All Chef Amatuers</h1>

<hr>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Specialty</th>
      <th>email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user.each do |user| %>
      <tr>
        <td><%= link_to user.name, recipe_path  %></td>
        <td><%= user.specialty %></td>
        <td><%= user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>
